<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TestActivity">

    <Button
        android:id="@+id/bt_choose_proxy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="选择代理"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bt_proxy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="代理测试"
        app:layout_constraintEnd_toEndOf="@+id/bt_choose_proxy"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/bt_choose_proxy"
        app:layout_constraintTop_toBottomOf="@+id/bt_choose_proxy" />

    <Button
        android:id="@+id/bt_task"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="开始任务"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/bt_proxy"
        app:layout_constraintStart_toEndOf="@+id/bt_proxy"
        app:layout_constraintTop_toTopOf="@+id/bt_proxy" />

    <TextView
        android:id="@+id/tv_tip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tip"
        android:textColor="@android:color/holo_red_light"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/textView"
        tools:text="tip" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="提示："
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/bt_choose_device"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/bt_choose_device"
        app:layout_constraintTop_toBottomOf="@+id/bt_choose_device" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:lineSpacingExtra="18sp"
        android:text="经纬度："
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/tv_tip"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/tv_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0,0"
        android:textColor="@android:color/holo_red_light"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <Button
        android:id="@+id/bt_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="查询经纬度"
        app:layout_constraintBottom_toBottomOf="@+id/bt_proxy"
        app:layout_constraintStart_toEndOf="@+id/bt_proxy"
        app:layout_constraintTop_toTopOf="@+id/bt_proxy" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="当前IP："
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/tv_location"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/tv_ip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="未知"
        android:textColor="@android:color/holo_red_light"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView4"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/textView4" />

    <Button
        android:id="@+id/bt_ip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="查询IP"
        app:layout_constraintBottom_toBottomOf="@+id/bt_location"
        app:layout_constraintStart_toEndOf="@+id/bt_location"
        app:layout_constraintTop_toTopOf="@+id/bt_location" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="地址："
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_ip"
        app:layout_constraintStart_toEndOf="@+id/tv_ip"
        app:layout_constraintTop_toTopOf="@+id/tv_ip" />

    <TextView
        android:id="@+id/tv_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="未知"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView3" />

    <Button
        android:id="@+id/bt_choose_device"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="选择设备信息"
        app:layout_constraintEnd_toEndOf="@+id/bt_proxy"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/bt_proxy"
        app:layout_constraintTop_toBottomOf="@+id/bt_proxy" />

    <Button
        android:id="@+id/bt_one_key"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="一键改机和IP"
        app:layout_constraintBottom_toBottomOf="@+id/bt_choose_device"
        app:layout_constraintStart_toEndOf="@+id/bt_choose_device"
        app:layout_constraintTop_toTopOf="@+id/bt_choose_device"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/bt_wechat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="微信"
        app:layout_constraintBottom_toBottomOf="@+id/bt_choose_proxy"
        app:layout_constraintStart_toEndOf="@+id/bt_choose_proxy"
        app:layout_constraintTop_toTopOf="@+id/bt_choose_proxy"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/bt_douyin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="抖音"
        app:layout_constraintBottom_toBottomOf="@+id/bt_wechat"
        app:layout_constraintStart_toEndOf="@+id/bt_wechat"
        app:layout_constraintTop_toTopOf="@+id/bt_wechat"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="设备信息key："
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/tv_device_key"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="未选择"
        android:textColor="@android:color/holo_red_light"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/textView5" />

</androidx.constraintlayout.widget.ConstraintLayout>